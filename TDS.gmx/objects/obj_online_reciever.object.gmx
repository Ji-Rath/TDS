            <string>///Recieve Messages from Server

while(1)
{
    msize = receivemessage(global.tcp); //Reads size of message
    
    //If size is 0 then we disconnected
    if(msize == 0)
    {
        show_message("The connection to the server has been lost");
        room_goto(rm_menu);
    }
    
    //If the size is less than 0, then there is no messages... So use break
    //to stop the code from continuing
    if(msize &lt;= 0) break;
    
    //If there is a message....
    mid = readbyte();
    
    //Execute different scripts for different online messages
    
    switch(mid)
    {
        //If the message was MOVEMENT
        case MSG_MOVEMENT:
        //Call movement script
        case_msg_movement()
        break;
        
          
    }
}
</string>
